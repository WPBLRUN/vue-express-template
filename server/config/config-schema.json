        "MONGO",
        "LOG",
        "DBUSER",
        "DBPASSWORD",
        "REDIS",
        "PORT",
        "USERSERVER",
        "FILESERVER",
        "DMPSERVER",
        "BOXAPISERVER",
        "MAILSERVER"
    ],
