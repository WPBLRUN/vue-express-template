            "type": "string"
        },
        "LOG": {
            "enum": [
                "info",
                "error",
                "verbose",
                "debug"
            ]
        },
        "DBUSER": {
            "type": "string"
        },
        "DBPASSWORD": {
            "type": "string"
        },
        "REDIS": {
            "type": "string"
        },
        "PORT": {
            "type": "string"
        },
        "USERSERVER": {
            "type": "string"
        },
        "FILESERVER": {
            "type": "string"
        },
        "DMPSERVER": {
            "type": "string"
        },
        "BOXAPISERVER": {
            "type": "string"
        },
        "MAILSERVER": {
            "type": "string"
        }
    }
}
